# GreendoorAI API Configuration
# Replace these with your actual API endpoints and keys

# API Base URL - Replace with actual GreendoorAI API endpoint
VITE_GREENDOOR_API_URL=https://api.greendoorai.com/v1

# API Key - Replace with actual API key
VITE_GREENDOOR_API_KEY=your_api_key_here

# Feature Flags
VITE_ENABLE_REAL_TIME_UPDATES=true
VITE_ENABLE_AGENT_DASHBOARD=true
VITE_ENABLE_PERFORMANCE_METRICS=true

# Development Settings
VITE_DEBUG_MODE=true
VITE_MOCK_API_DELAY=2000
